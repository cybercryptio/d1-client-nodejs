{
  "name": "@cybercryptio/d1-client-nodejs",
  "version": "0.0.1",
  "description": "",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "repository": "https://github.com/cybercryptio/d1-client-nodejs.git",
  "scripts": {
    "test": "jest",
    "build": "npm run protobuf && tsc --build --clean && tsc --build && cp -f src/proto.d.ts dist/src/",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "protobuf": "pbjs -t static-module -w commonjs -o src/proto.js protobufs/*.proto && pbts -o src/proto.d.ts src/proto.js",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "encryption",
    "crypto",
    "cryptography",
    "post-quantum"
  ],
  "author": "CyberCrypt",
  "license": "Apache-2.0",
  "devDependencies": {
    "@tsconfig/node16": "^1.0.3",
    "@types/jest": "^28.1.6",
    "jest": "^28.1.3",
    "prettier": "^2.7.1",
    "protobufjs-cli": "~1.0.0",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.23.11",
    "typedoc-plugin-markdown": "^3.13.4",
    "typescript": "^4.7.4"
  },
  "files": [
    "dist/src/**/*"
  ],
  "dependencies": {
    "@grpc/grpc-js": "^1.6.8",
    "protobufjs": "~7.0.0"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  }
}