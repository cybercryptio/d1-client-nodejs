  "devDependencies": {
    "@tsconfig/node16": "^1.0.3",
    "prettier": "^2.7.1",
    "protobufjs-cli": "~1.0.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.7.4"
  },
  "files": [
    "dist/**/*"
  ],
  "dependencies": {
    "@grpc/grpc-js": "^1.6.8",
    "protobufjs": "~7.0.0"
  }
}
