{
  "name": "d1-client-nodejs",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run protobuf && tsc --build && cp -f src/proto.d.ts dist",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
